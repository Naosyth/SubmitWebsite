<table class="submission-output" id="run_input_<%= run_save.id %>">
  <thead>
    <tr>
      <th colspan=2><%= run_save.input.name %></th>
    </tr>
    <tr>
      <td colspan=2><%= run_save.input.description %><%= raw "<br>Hidden To Students" if not run_save.input.student_visible %></td>
    </tr>
  </thead>
  <tbody>
    <tr>
      <th colspan=2>Input Data</th>
    </tr>
    <tr>
      <td colspan=2><%= run_save.input.data %></td>
    </tr>
    <tr>
      <th>Your Output</th>
      <th>Expected Output</th>
    </tr>
    <tr>
      <td><%= run_save.output %></td>
      <td><%= run_save.input.output %></td>
    </tr>
    <tr>
      <th colspan=2>Difference</th>
    </tr>
    <tr>
      <td colspan=2><%= run_save.difference %></td>
    </tr>
  </tbody>
</table>
